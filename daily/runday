#!/bin/sh

if [ x$1 = x ]; then
	echo "I need to know what project to work on!!"
	exit 1
fi

psql -v ProjectID=$1 -f retire.sql
psql -v ProjectID=$1 -f newjoin.sql
psql -v ProjectID=$1 -f dy_appendday.sql
psql -v ProjectID=$1 -f em_update.sql
psql -v ProjectID=$1 -f em_rank.sql
psql -v ProjectID=$1 -f tm_update.sql
psql -v ProjectID=$1 -f tm_rank.sql
psql -v ProjectID=$1 -f platform.sql
psql -v ProjectID=$1 -f dy_dailyblocks.sql
sudo pcpages
psql -v ProjectID=$1 -f audit.sql
